###  http://EncodingActs.github.io/annotated/MAon/data_format
mao:data_format rdf:type owl:DatatypeProperty ;
                rdfs:subPropertyOf mao:dataProperties ;
                rdf:type owl:FunctionalProperty ;
                rdfs:domain mao:class:MA_book ;
                rdfs:range mao:Image ,
                           mao:ManualBook ,
                           mao:MoCap ,
                           mao:Texts ,
                           mao:Video ;
