###  http://EncodingActs.github.io/MAont/MAont_base#E21_Person
MA_ont:E21_Person rdf:type owl:Class ;
                      rdfs:subClassOf owl:Thing ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty MA_ont:creates ;
                                        owl:someValuesFrom MA_ont:E22_Man-made_object
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty MA_ont:identified_by ;
                                        owl:someValuesFrom MA_ont:E41_Appellation
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty MA_ont:lives_in ;
                                        owl:someValuesFrom MA_ont:E52_Time-Span
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty MA_ont:resides_in ;
                                        owl:someValuesFrom MA_ont:E53_Place
                                      ] ;
                      rdfs:label "E21. Person" .


