###  http://EncodingActs.github.io/annotated/MAon/adapts
mao:adapts rdf:type owl:ObjectProperty ;
           rdfs:domain mao:class:MA_Master ;
           rdfs:range mao:class:MA_style ,
                      mao:class:MA_syllabus ,
                      mao:class:armament ,
                      mao:class:training_method ,
                      mao:class:training_tool ;
           mao:ontoMA_name_zh "修改"@zh-hk ;
           rdfs:comment "The adapts property refers to the relation that a Master adapts certain (existing) techniques, training methods like forms and form sets, weapons, training tools, or even a style or system."@en ;
           rdfs:label "adapts" .


###  http://EncodingActs.github.io/annotated/MAon/adopts
mao:adopts rdf:type owl:ObjectProperty ;
           rdfs:domain mao:class:MA_institute ,
                       mao:class:MA_style ,
                       mao:class:MA_syllabus ,
                       mao:class:MA_system ;
           rdfs:range mao:class:training_method ;
           mao:ontoMA_name_zh "采用"@zh-hk ;
           rdfs:comment "The property adopts is essentially the verb adopt, signifying that a martial arts system or style adopts a training method, or a transmission institute or syllabus adopts a training method."@en ;
           rdfs:label "adopts" .


###  http://EncodingActs.github.io/annotated/MAon/belongs_to
mao:belongs_to rdf:type owl:ObjectProperty ;
               rdfs:domain mao:class:MA_style ,
                           mao:class:MA_system ;
               rdfs:range mao:class:MA_system ;
               rdfs:label "belongs_to" .


###  http://EncodingActs.github.io/annotated/MAon/creates
mao:creates rdf:type owl:ObjectProperty ;
            rdfs:domain mao:class:MA_Master ;
            rdfs:range mao:class:MA_style ,
                       mao:class:MA_syllabus ,
                       mao:class:armament ,
                       mao:class:training_method ,
                       mao:class:training_tool ;
            mao:ontoMA_name_zh "創造"@zh-hk ;
            mao:ontoMA_otherName_zh "創建"@zh-hk ;
            rdfs:comment "The creates property refers to the relation that a Master creates certain (new) techniques, training methods like forms and form sets, weapons, training tools, or even a style or system."@en ;
            rdfs:label "creates" .


###  http://EncodingActs.github.io/annotated/MAon/developed_from
mao:developed_from rdf:type owl:ObjectProperty ;
                   rdfs:domain mao:class:MA_style ;
                   rdfs:range mao:class:MA_style ;
                   rdfs:label "developed_from"@en .


###  http://EncodingActs.github.io/annotated/MAon/documents
mao:documents rdf:type owl:ObjectProperty ;
              rdfs:domain mao:class:MA_book ;
              rdfs:range mao:class:MA_Master ,
                         mao:class:MA_institute ,
                         mao:class:MA_style ,
                         mao:class:MA_system ,
