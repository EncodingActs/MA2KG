###  https://purl.org/maont#E21_Person
ontologies:E21_Person rdf:type owl:Class ;
                      rdfs:subClassOf owl:Thing ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty ontologies:creates ;
                                        owl:someValuesFrom ontologies:E22_Man-made_object
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty ontologies:identified_by ;
                                        owl:someValuesFrom ontologies:E41_Appellation
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty ontologies:lives_in ;
                                        owl:someValuesFrom ontologies:E52_Time-Span
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty ontologies:resides_in ;
                                        owl:someValuesFrom ontologies:E53_Place
                                      ] ;
                      rdfs:label "E21. Person" .


